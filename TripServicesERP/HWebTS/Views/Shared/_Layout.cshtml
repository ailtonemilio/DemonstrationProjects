<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>
        @if (!string.IsNullOrWhiteSpace(ViewBag.Title))
        {
            @ViewBag.Title@:-Trip Services
        }

    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    @*<link rel="shortcut icon" href="~/Ubold/assets/images/favicon.ico">*@

    <!-- Custom box css -->
    <link href="~/Ubold/assets/libs/custombox/custombox.min.css" rel="stylesheet">

    <!-- Plugins css -->
    <link href="~/Ubold/assets/libs/flatpickr/flatpickr.min.css" rel="stylesheet" type="text/css" />

    <!-- Sweet Alert-->
    <link href="~/Ubold/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />

    <!-- Jquery Toast css -->
    <link href="~/Ubold/assets/libs/jquery-toast/jquery.toast.min.css" rel="stylesheet" type="text/css" />

    <!-- Loading button css -->
    <link href="~/Ubold/assets/libs/ladda/ladda-themeless.min.css" rel="stylesheet" type="text/css" />

    <!-- third party css -->
    <link href="~/Ubold/assets/libs/datatables/dataTables.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/Ubold/assets/libs/datatables/responsive.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/Ubold/assets/libs/datatables/buttons.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/Ubold/assets/libs/datatables/select.bootstrap4.css" rel="stylesheet" type="text/css" />
    <!-- third party css end -->

    <!-- Animation css -->
    <link href="Ubold/assets/libs/animate/animate.min.css" rel="stylesheet" type="text/css" />

    <!-- App css -->
    <link href="~/Ubold/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Ubold/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Ubold/assets/css/app.min.css" rel="stylesheet" type="text/css" />

    @* Is necessary this links here in all index *@
    @*<link href="~/Scripts/sweetalert/sweetalert.css" rel="stylesheet" />
        <script src="~/Scripts/sweetalert/sweetalert.js"></script>
        <script src="~/Scripts/sweetalert/sweetalert.min.js"></script>*@

</head>

<body>

    <!-- Begin page -->
    <div id="wrapper">
        <!-- Topbar Start -->
        <div class="navbar-custom bg-dark">
            <ul class="list-unstyled topnav-menu float-right mb-0">
                @*<li class="d-none d-sm-block">
                        <form class="app-search">
                            <div class="app-search-box">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Search...">
                                    <div class="input-group-append">
                                        <button class="btn" type="submit">
                                            <i class="fe-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </li>*@

                <li class="dropdown notification-list">
                    <a class="nav-link dropdown-toggle  waves-effect waves-light" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <i class="fe-bell noti-icon"></i>
                        <span class="badge badge-danger rounded-circle noti-icon-badge">9</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right dropdown-lg">
                        <!-- item-->
                        <div class="dropdown-item noti-title">
                            <h5 class="m-0">
                                <span class="float-right">
                                    <a href="" class="text-dark">
                                        <small>Clear All</small>
                                    </a>
                                </span>Notification
                            </h5>
                        </div>
                        <div class="slimscroll noti-scroll">
                            <!-- item-->
                            <a href="javascript:void(0);" class="dropdown-item notify-item active">
                                <div class="notify-icon">
                                    <img src="~/Ubold/assets/images/users/user-1.jpg" class="img-fluid rounded-circle" alt="" />
                                </div>
                                <p class="notify-details">Cristina Pride</p>
                                <p class="text-muted mb-0 user-msg">
                                    <small>Hi, How are you? What about our next meeting</small>
                                </p>
                            </a>
                            <!-- item-->
                            <a href="javascript:void(0);" class="dropdown-item notify-item">
                                <div class="notify-icon bg-primary">
                                    <i class="mdi mdi-comment-account-outline"></i>
                                </div>
                                <p class="notify-details">
                                    Caleb Flakelar commented on Admin
                                    <small class="text-muted">1 min ago</small>
                                </p>
                            </a>
                            <!-- item-->
                            <a href="javascript:void(0);" class="dropdown-item notify-item">
                                <div class="notify-icon">
                                    <img src="~/Ubold/assets/images/users/user-4.jpg" class="img-fluid rounded-circle" alt="" />
                                </div>
                                <p class="notify-details">Karen Robinson</p>
                                <p class="text-muted mb-0 user-msg">
                                    <small>Wow ! this admin looks good and awesome design</small>
                                </p>
                            </a>
                            <!-- item-->
                            <a href="javascript:void(0);" class="dropdown-item notify-item">
                                <div class="notify-icon bg-warning">
                                    <i class="mdi mdi-account-plus"></i>
                                </div>
                                <p class="notify-details">
                                    New user registered.
                                    <small class="text-muted">5 hours ago</small>
                                </p>
                            </a>
                            <!-- item-->
                            <a href="javascript:void(0);" class="dropdown-item notify-item">
                                <div class="notify-icon bg-info">
                                    <i class="mdi mdi-comment-account-outline"></i>
                                </div>
                                <p class="notify-details">
                                    Caleb Flakelar commented on Admin
                                    <small class="text-muted">4 days ago</small>
                                </p>
                            </a>
                            <!-- item-->
                            <a href="javascript:void(0);" class="dropdown-item notify-item">
                                <div class="notify-icon bg-secondary">
                                    <i class="mdi mdi-heart"></i>
                                </div>
                                <p class="notify-details">
                                    Carlos Crouch liked
                                    <b>Admin</b>
                                    <small class="text-muted">13 days ago</small>
                                </p>
                            </a>
                        </div>
                        <!-- All-->
                        <a href="javascript:void(0);" class="dropdown-item text-center text-primary notify-item notify-all">
                            View all
                            <i class="fi-arrow-right"></i>
                        </a>
                    </div>
                </li>
                <li class="dropdown notification-list">
                    @Html.Partial("PartialViewsV2/_UserInfoV2")
                </li>
                @*<li class="dropdown notification-list">
                        <a href="javascript:void(0);" class="nav-link right-bar-toggle waves-effect waves-light">
                            <i class="fe-settings noti-icon"></i>
                        </a>
                    </li>*@

            </ul>
            <!-- LOGO -->
            @Html.Partial("PartialViewsV2/_Logo")
            <ul class="list-unstyled topnav-menu topnav-menu-left m-0">
                <li>
                    <button class="button-menu-mobile waves-effect waves-light">
                        <i class="fe-menu"></i>
                    </button>
                </li>
            </ul>
        </div>
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left-side-menu">
            <div class="slimscroll-menu">

                <!-- User box -->
                @*@Html.Partial("PartialViewsV2/_UserMenuBox")*@

                <!--- Sidemenu -->
                @Html.Partial("PartialViewsV2/_Menu")
                <!-- End Sidebar -->
                <div class="clearfix"></div>
            </div>
            <!-- Sidebar -left -->
        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">

                <!-- Start Content-->
                <div class="container-fluid">

                    @RenderBody()

                </div> <!-- container -->

            </div> <!-- content -->
            <!-- Footer Start -->
            <footer class="footer">
                @Html.Partial("PartialViewsV2/_Footer")
            </footer>
            <!-- end Footer -->

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- Right Sidebar -->
    <div class="right-bar">
        <div class="rightbar-title">
            <a href="javascript:void(0);" class="right-bar-toggle float-right">
                <i class="dripicons-cross noti-icon"></i>
            </a>
            <h5 class="m-0 text-white">Settings</h5>
        </div>
        <div class="slimscroll-menu">
            <!-- User box -->
            <div class="user-box">
                <div class="user-img">
                    <img src="~/Ubold/assets/images/users/user-1.jpg" alt="user-img" title="Mat Helme" class="rounded-circle img-fluid">
                    <a href="javascript:void(0);" class="user-edit"><i class="mdi mdi-pencil"></i></a>
                </div>

                <h5><a href="javascript: void(0);">Geneva Kennedy</a> </h5>
                <p class="text-muted mb-0"><small>Admin Head</small></p>
            </div>
            <!-- Settings -->
            <hr class="mt-0" />
            <h5 class="pl-3">Basic Settings</h5>
            <hr class="mb-0" />
            <div class="p-3">
                <div class="checkbox checkbox-primary mb-2">
                    <input id="Rcheckbox1" type="checkbox" checked>
                    <label for="Rcheckbox1">
                        Notifications
                    </label>
                </div>
                <div class="checkbox checkbox-primary mb-2">
                    <input id="Rcheckbox2" type="checkbox" checked>
                    <label for="Rcheckbox2">
                        API Access
                    </label>
                </div>
                <div class="checkbox checkbox-primary mb-2">
                    <input id="Rcheckbox3" type="checkbox">
                    <label for="Rcheckbox3">
                        Auto Updates
                    </label>
                </div>
                <div class="checkbox checkbox-primary mb-2">
                    <input id="Rcheckbox4" type="checkbox" checked>
                    <label for="Rcheckbox4">
                        Online Status
                    </label>
                </div>
                <div class="checkbox checkbox-primary mb-0">
                    <input id="Rcheckbox5" type="checkbox" checked>
                    <label for="Rcheckbox5">
                        Auto Payout
                    </label>
                </div>
            </div>
            <!-- Timeline -->
            <hr class="mt-0" />
            <h5 class="pl-3 pr-3">Messages <span class="float-right badge badge-pill badge-danger">25</span></h5>
            <hr class="mb-0" />
            <div class="p-3">
                <div class="inbox-widget">
                    <div class="inbox-item">
                        <div class="inbox-item-img"><img src="~/Ubold/assets/images/users/user-2.jpg" class="rounded-circle" alt=""></div>
                        <p class="inbox-item-author"><a href="javascript: void(0);" class="text-dark">Tomaslau</a></p>
                        <p class="inbox-item-text">I've finished it! See you so...</p>
                    </div>
                    <div class="inbox-item">
                        <div class="inbox-item-img"><img src="~/Ubold/assets/images/users/user-3.jpg" class="rounded-circle" alt=""></div>
                        <p class="inbox-item-author"><a href="javascript: void(0);" class="text-dark">Stillnotdavid</a></p>
                        <p class="inbox-item-text">This theme is awesome!</p>
                    </div>
                    <div class="inbox-item">
                        <div class="inbox-item-img"><img src="~/Ubold/assets/images/users/user-4.jpg" class="rounded-circle" alt=""></div>
                        <p class="inbox-item-author"><a href="javascript: void(0);" class="text-dark">Kurafire</a></p>
                        <p class="inbox-item-text">Nice to meet you</p>
                    </div>
                    <div class="inbox-item">
                        <div class="inbox-item-img"><img src="~/Ubold/assets/images/users/user-5.jpg" class="rounded-circle" alt=""></div>
                        <p class="inbox-item-author"><a href="javascript: void(0);" class="text-dark">Shahedk</a></p>
                        <p class="inbox-item-text">Hey! there I'm available...</p>
                    </div>
                    <div class="inbox-item">
                        <div class="inbox-item-img"><img src="~/Ubold/assets/images/users/user-6.jpg" class="rounded-circle" alt=""></div>
                        <p class="inbox-item-author"><a href="javascript: void(0);" class="text-dark">Adhamdannaway</a></p>
                        <p class="inbox-item-text">This theme is awesome!</p>
                    </div>
                </div> <!-- end inbox-widget -->
            </div> <!-- end .p-3-->
        </div>
        <!-- end slimscroll-menu-->
    </div>
    <!-- /Right-bar -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>

    <!-- Vendor js -->
    <script src="~/Ubold/assets/js/vendor.min.js"></script>

    <!-- Plugins js-->
    <script src="~/Ubold/assets/libs/flatpickr/flatpickr.min.js"></script>
    <script src="~/Ubold/assets/libs/jquery-knob/jquery.knob.min.js"></script>
    <script src="~/Ubold/assets/libs/jquery-sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Ubold/assets/libs/flot-charts/jquery.flot.js"></script>
    <script src="~/Ubold/assets/libs/flot-charts/jquery.flot.time.js"></script>
    <script src="~/Ubold/assets/libs/flot-charts/jquery.flot.tooltip.min.js"></script>
    <script src="~/Ubold/assets/libs/flot-charts/jquery.flot.selection.js"></script>
    <script src="~/Ubold/assets/libs/flot-charts/jquery.flot.crosshair.js"></script>

    <!-- third party js -->
    <script src="~/Ubold/assets/libs/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/dataTables.bootstrap4.js"></script>
    <script src="~/Ubold/assets/libs/datatables/dataTables.responsive.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/responsive.bootstrap4.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/dataTables.buttons.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/buttons.bootstrap4.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/buttons.html5.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/buttons.flash.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/buttons.print.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/dataTables.keyTable.min.js"></script>
    <script src="~/Ubold/assets/libs/datatables/dataTables.select.min.js"></script>
    <script src="~/Ubold/assets/libs/pdfmake/pdfmake.min.js"></script>
    <script src="~/Ubold/assets/libs/pdfmake/vfs_fonts.js"></script>
    <!-- third party js ends -->
    <!-- Datatables init -->
    <script src="~/Ubold/assets/js/pages/datatables.init.js"></script>

    <!-- Dashboar 1 init js-->
    <script src="~/Ubold/assets/js/pages/dashboard-1.init.js"></script>

    <!-- Sweet Alerts js -->
    <script src="~/Ubold/assets/libs/sweetalert2/sweetalert2.min.js"></script>

    <!-- Tost-->
    <script src="~/Ubold/assets/libs/jquery-toast/jquery.toast.min.js"></script>

    <!-- toastr init js-->
    <script src="~/Ubold/assets/js/pages/toastr.init.js"></script>

    <!-- Loading buttons js -->
    <script src="~/Ubold/assets/libs/ladda/spin.js"></script>
    <script src="~/Ubold/assets/libs/ladda/ladda.js"></script>

    <!-- Buttons init js-->
    <script src="~/Ubold/assets/js/pages/loading-btn.init.js"></script>

    <!-- Modal-Effect -->
    <script src="~/Ubold/assets/libs/custombox/custombox.min.js"></script>

    <!-- App js-->
    <script src="~/Ubold/assets/js/app.min.js"></script>


